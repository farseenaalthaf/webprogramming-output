<!DOCTYPE html>
<html>
	<head>
	<title>Changing image and text</title>
	<style>
			img{
					max-width:100%;
					border:2px solid green;
					border-radius:50%;
					padding:5px;
					height:auto;
			}
			#p1{
					position:relative;
					top:950;
					left:630;
					font-size:20px;
			}
	</style>
		<script>
					function one()
					{
						document.getElementById("img").src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSGiyHhlBLrDY8OV1_SAPXxgLmqXsrRR_sVvw&s";
						document.getElementById("p1").innerHTML= "onmouseover";
						document.getElementById("p1").style.color= "violet";
					}
					function two()
					{
						document.getElementById("img").src="https://media.istockphoto.com/id/510795912/photo/india-gate.jpg?s=612x612&w=0&k=20&c=kZkdrEDXEtoLK6Qh8XPywc9VYV95mJXXcWLBxHftN_U=";
						document.getElementById("p1").innerHTML= "onmouseout";
						document.getElementById("p1").style.color= "green";
					}
		</script>
		</head>
		<body>
				<img src="tulip3.jpg" id = "img" onmouseover="one()" onmouseout="two()">
				<b><p id = "p1"></p></b>
		</body>
</html>